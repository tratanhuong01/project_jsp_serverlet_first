<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ProcessRegister</servlet-name>
        <servlet-class>controller.ProcessRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessLogin</servlet-name>
        <servlet-class>controller.ProcessLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessLogout</servlet-name>
        <servlet-class>controller.ProcessLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessViewFriend</servlet-name>
        <servlet-class>controller.ProcessViewFriend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessAddFriend</servlet-name>
        <servlet-class>controller.ProcessAddFriend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessCancelFriend</servlet-name>
        <servlet-class>controller.ProcessCancelFriend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessAccpRequest</servlet-name>
        <servlet-class>controller.ProcessAccpRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAvatar</servlet-name>
        <servlet-class>controller.UpdateAvatar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessBia</servlet-name>
        <servlet-class>controller.ProcessBia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessFriends</servlet-name>
        <servlet-class>controller.ProcessFriends</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessAbout</servlet-name>
        <servlet-class>controller.ProcessAbout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>okokokok</servlet-name>
        <servlet-class>dao.okokokok</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProcessRegister</servlet-name>
        <url-pattern>/dang-nhap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessLogin</servlet-name>
        <url-pattern>/logged</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessLogout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessViewFriend</servlet-name>
        <url-pattern>/acc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessAddFriend</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessCancelFriend</servlet-name>
        <url-pattern>/cancel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessAccpRequest</servlet-name>
        <url-pattern>/accp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAvatar</servlet-name>
        <url-pattern>/UpdateAvatar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessBia</servlet-name>
        <url-pattern>/ProcessBia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessFriends</servlet-name>
        <url-pattern>/friends</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessAbout</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>okokokok</servlet-name>
        <url-pattern>/akaka</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
